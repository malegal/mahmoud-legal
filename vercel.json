{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.json",
      "use": "@vercel/static"
    },
    {
      "src": "images/*",
      "use": "@vercel/static"
    },
    {
      "src": "blog/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/admin$",
      "dest": "/admin.html",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "src": "^/client-inquiry$",
      "dest": "/client-inquiry.html"
    },
    {
      "src": "^/blog$",
      "dest": "/blog.html"
    },
    {
      "src": "^/blog/(.*)$",
      "dest": "/blog/$1"
    },
    {
      "src": "^/images/(.*)$",
      "dest": "/images/$1"
    },
    {
      "src": "^/(.*\\.(css|js|json|png|jpg|jpeg|gif|ico|svg|webp))$",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      }
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/admin",
      "destination": "/admin.html"
    },
    {
      "source": "/client-inquiry",
      "destination": "/client-inquiry.html"
    }
  ],
  "redirects": [
    {
      "source": "/dashboard",
      "destination": "/admin",
      "permanent": false
    },
    {
      "source": "/track",
      "destination": "/client-inquiry",
      "permanent": false
    }
  ],
  "trailingSlash": false,
  "cleanUrls": true,
  "framework": null,
  "outputDirectory": null,
  "installCommand": null,
  "devCommand": null,
  "buildCommand": null
}
